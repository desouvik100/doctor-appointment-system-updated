# Production Environment Variables
# NOTE: Set these in Vercel Dashboard, not here!

# API Configuration - Your Render backend URL
REACT_APP_API_URL=https://your-backend.onrender.com
REACT_APP_BACKEND_URL=https://your-backend.onrender.com

# Stripe Configuration (set in Vercel dashboard)
REACT_APP_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key

# Payment Mode
REACT_APP_USE_STRIPE_PAYMENTS=false

# Google OAuth Configuration (set in Vercel dashboard)
REACT_APP_GOOGLE_CLIENT_ID=your_google_client_id

# Environment
NODE_ENV=production
GENERATE_SOURCEMAP=false
CI=false
DISABLE_ESLINT_PLUGIN=true
